#include <unistd.h>
void ft_putchar(int d)
{
	write(1, &d, 1);
}
void ft_print_comb(void)
{
	int i;
	int j;
	int k;
	i = 0;
	while(i <=0 )
	{
		j = 1;
		j++;
		if(j <= 1)
		{
			k = 2;
			k++;
			else if(k <= 2)
			{
				ft_putchar(i + '0');
				ft_putchar(j + '1');
				ft_putchar(k + '2');
				ft_putchar(',');
				ft_putchar(' ');
			}
		}
	}
}
int main()
{
	ft_putchar();
	return 0;
}
